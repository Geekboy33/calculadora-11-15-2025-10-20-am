{
  "name": "ledgerdaesterminal",
  "private": true,
  "version": "1.1.0",
  "description": "LedgerDAESTerminal - Digital Commercial Bank Ltd",
  "author": "DAES Team",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "server:api1": "node server/api1-server.js",
    "server:arb": "node server/defi-arb-api.js",
    "server:dusd": "cd server && npm run dev",
    "dev:all": "concurrently -k -n APP,API -c green,cyan \"vite\" \"cd server && npx tsx src/index.ts\"",
    "dev:dusd": "concurrently -k -n APP,DUSD -c green,yellow \"vite\" \"cd server && npm run dev\"",
    "dev:full": "concurrently -k -n APP,API,API1 -c green,cyan,magenta \"vite\" \"node server/index.js\" \"node server/api1-server.js\"",
    "dev:arb": "concurrently -k -n APP,API,ARB -c green,cyan,yellow \"vite\" \"node server/index.js\" \"node server/defi-arb-bot-real.js\"",
    "bot:live": "concurrently -k -n APP,BOT -c green,yellow \"vite\" \"node server/arb-bot-fast.js\"",
    "bot:scalper": "concurrently -k -n APP,SCALPER -c green,red \"vite\" \"node server/arb-bot-scalper.js\"",
    "bot:test": "concurrently -k -n APP,API -c green,yellow \"vite\" \"node server/arb-api.js\"",
    "bot:contracts": "concurrently -k -n APP,BOT -c green,magenta \"vite\" \"node server/arb-bot-with-contracts.js\"",
    "bot:profit": "concurrently -k -n APP,BOT -c green,cyan \"vite\" \"node server/arb-bot-profit-only.js\"",
    "bot:flash": "concurrently -k -n APP,FLASH -c green,red \"vite\" \"node server/arb-bot-flash-loan-executor.js\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "xcp:remit": "tsx examples/xcp-remit-example.ts",
    "xcp:test": "tsx examples/xcp-remit-example.ts",
    "electron": "electron .",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:4000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:build:win": "npm run build && electron-builder --win",
    "electron:build:mac": "npm run build && electron-builder --mac",
    "electron:build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.1",
    "alchemy-sdk": "^3.6.5",
    "axios": "^1.13.2",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "jszip": "^3.10.1",
    "lucide-react": "^0.344.0",
    "lz-string": "^1.5.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.17.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.1",
    "web3": "^4.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.19.23",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "electron-updater": "^6.3.9",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "ethers": "^6.16.0",
    "globals": "^15.9.0",
    "hardhat": "^3.1.2",
    "postcss": "^8.4.35",
    "solc": "^0.8.33",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.1.0",
    "wait-on": "^9.0.3",
    "workbox-window": "^7.4.0"
  }
}
